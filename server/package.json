{
    "name": "@graphql-ts-app-example/server",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "check-types": "tsc --noEmit",
        "build": "nest build",
        "start": "yarn prepare && nest start",
        "start:dev": "yarn prepare && nest start --watch",
        "start:debug": "yarn prepare && nest start --debug --watch",
        "start:prod": "yarn prepare && node dist/main",
        "prepare": "cat src/sql/schema.sql src/sql/seed.sql src/sql/custom.sql | sqlite3 database.db"
    },
    "dependencies": {
        "@apollo/server": "^4.11.0",
        "@nestjs/apollo": "^12.2.0",
        "@nestjs/common": "^10.4.5",
        "@nestjs/core": "^10.4.5",
        "@nestjs/graphql": "^12.2.0",
        "@nestjs/platform-express": "^10.4.5",
        "graphql": "^16.9.0",
        "knex": "^3.1.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "sqlite3": "^5.1.7",
        "ts-node": "^10.9.2"
    },
    "devDependencies": {
        "@graphql-ts-app-example/ts-config": "workspace:*",
        "@nestjs/cli": "^10.4.5",
        "@nestjs/schematics": "^10.2.0",
        "@nestjs/testing": "^10.4.5",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.13",
        "@types/node": "^22.7.6",
        "jest": "^29.7.0",
        "typescript": "^5.6.3"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": "^22"
    },
    "packageManager": "yarn@4.4.1"
}
